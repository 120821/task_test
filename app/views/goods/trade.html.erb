<script>
  // 用于填充柱状图的数据
  var data = [
    { name: '进场蔬菜总量', y: 50 },
    { name: '出场蔬菜总量', y: 70 }
  ];

  // 创建柱状图
  var chart = Highcharts.chart('chart_supply_index', {
    chart: {
      type: 'column'
    },
    title: {
      text: null //''
    },
    exporting: {
      enabled: false // 设置为false表示禁用导出功能
    },
    xAxis: {
      categories: ['进场蔬菜总量', '出场蔬菜总量']
    },
    yAxis: {
      title: {
        text: '单位：吨'
      },
      plotLines: [{ // 添加数量提示
        color: '#ccc',
        dashStyle: 'dot',
        width: 1,
        value: 60, // 数量提示值
        zIndex: 3
      }]
    },
    tooltip: { // 添加悬浮提示
      formatter: function() {
        return '<b>' + this.point.name + '</b><br/>' + this.y + ' 吨';
      }
    },
    series: [{
      data: data,
      name: '总量',// 添加名字
      color: '#165DFF', // 添加背景色
      dataLabels: {
        enabled: true,
        inside: true,
        color: '#fff',
        verticalAlign: 'top',
        formatter: function() {
          return this.y;
        }
      }
    }]
  });
</script>
