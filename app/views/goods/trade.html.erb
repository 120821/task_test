<div class="cards_supply">
  <div class="card_supply" style="width: 387px;">
    <p class="card_supply_title" style="width: 387px;">蔬菜交易市场监控</p>
    <p><span class="card_integer">268</span><span style="margin-left: 6px;">家</span></p>
    <p class="data_time">
      较昨日<span class="data_up" style="color: #d84644; margin-left: 6px; ">12家</span>
      <%= image_tag 'rate_up.png', style: 'margin-top: -10px;' %>
      <span style="margin-left: 47px;">较上周</span><span class="data_down" style="color: #5fb136;">6家</span>
      <%= image_tag 'rate_down.png', style:"margin-top: -10px;" %>
    <p>

  </div>
  <div class="card_supply" style="width: 387px; margin-left: 20px;">
    <p class="card_supply_title">免收进场费（总额）</p>
    <p><span class="card_integer">30.10</span><span style="margin-left: 6px;">万元</span></p>

    <p class="data_time">
      较昨日<span class="data_up" style="color: #d84644;margin-left: 6px; ">1.2万元</span>
      <%= image_tag 'rate_up.png', style: 'margin-top: -10px;' %>
      <span style="margin-left: 47px;">较上周</span><span class="data_down" style="color: #5fb136;">1.0万元</span>
      <%= image_tag 'rate_down.png', style:"margin-top: -10px;" %>
    <p>
  </div>
  <div class="card_supply" style="margin-left: 20px;width: 387px;">
    <p class="card_supply_title">免收出场费（总额）</p>
    <p><span class="card_integer">300</span><span style="margin-left: 6px;">万元</span></p>
    <p class="data_time">
      较昨日<span class="data_up" style="color: #d84644;margin-left: 6px; ">1.2万元</span>
      <%= image_tag 'rate_up.png', style: 'margin-top: -10px;' %>
      <span style="margin-left: 47px;">较上周</span><span class="data_down" style="color: #5fb136;">1.0万元</span>
      <%= image_tag 'rate_down.png', style:"margin-top: -10px;" %>
    <p>
  </div>
</div>
<div class="select_supply">
  <p class="supply_select_title">日交易量及进出场费用</p>
  <%= form_tag '' do %>
    <!--
    地州: <%= select_tag :region, options_for_select(REGIONS_WITH_NIL, params[:region]), class: 'select_new' %>
    品类: <%= select_tag :sales_inventory_situation, options_for_select(CATEGORY_ARRAY, params[:sales_inventory_situation]), class: 'select_new' %>
    -->
    <%= text_field_tag :daily_goods_date, params[:daily_goods_date], id: 'datepicker', placeholder: '请选择日期' %>
    <%= submit_tag "查询", class: "form_submit", class: 'btn btn-primary btn-sm', style: 'margin-left: 20px;' %>
  <% end %>
</div>

