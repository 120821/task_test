  .links-container a {
    width: 68px;
    height: 32px;
    background: #FFFFFF;
    border-radius: 2px 0px 0px 2px;
    opacity: 1;
    border: 1px solid #D9D9D9;

  }

  .links_active{
    color: #006DF1;
    opacity: 1;
    border: 1px solid #006DF1;
  }
  .links-container a.active,
/*.links-container a:hover */
  {
    color: blue;
    opacity: 1;
    border: 1px solid #006DF1;

  }

  select {
  color: red; /* 定义选中选项文字颜色为红色 */
  background-color: #eee; /* 定义背景颜色为浅灰色 */
}
