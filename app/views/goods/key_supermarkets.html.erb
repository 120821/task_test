<script src="https://code.highcharts.com/highcharts.js"></script>
<div class="card_div">
  <div class="card_index">
    <p class="every_card">全区重点商超</p>
    <span class="data_index">268<span class="data_unit" style="margin-left: 6px;">家</span></span>
    <p class="data_time">
      较上周<span style="margin-left: 6px; color: #d84644;">12家</span>
      <%= image_tag 'rate_up.png', style: 'margin-top: -10px;' %>
      <span style="margin-left: 47px;">较上月</span>
      <span class="" style="margin-left: 6px; color: #5fb136;">8家</span>
      <%= image_tag 'rate_down.png', style:"margin-top: -10px;" %>
    <p>
  </div>
  <div class="card_index">
    <p class="every_card">开展营业</p>
    <p class="data_index">300<span class="data_unit" style="margin-left: 6px;">万元</span></p>
    <p class="data_time">较昨日<span class="data_up" style="margin-left: 6px;color: #d84644;">12家</span>
      <%= image_tag 'rate_up.png', style: 'margin-top: -10px;' %>
      <span style="margin-left: 47px;">较上周</span><span class="data_down" style="margin-left: 6px;color: #5fb136;">2家</span>
      <%= image_tag 'rate_down.png', style: 'margin-top: -10px;' %>
    <p>
  </div>
  <div class="card_index">
    <p class="every_card">未开展营业</p>
    <p class="data_index">300<span class="data_unit" style="margin-left: 6px;">家</span>
    </p>
    <p class="data_time">较昨日<span class="data_up" style="margin-left: 6px;color: #d84644;">12家</span>
      <%= image_tag 'rate_up.png', style: 'margin-top: -10px;' %>
    <span style="margin-left: 47px;">较上周</span><span class="data_down" style='margin-left: 6px;color: #5fb136'>2家</span>
      <%= image_tag 'rate_down.png', style: 'margin-top: -10px;' %>
    <p>
  </div>
</div>
<div class="card_div_store">
  <div class="card_store">
    <div class="picture_danger">
    </div>
    <div class="data_danger">
      <span class="every_card_store">在17种商品中</span></br>
      <span class="data_index_store_danger">品类预警<span class="store_number">2</span><span class="data_unit" style="margin-left: 6px;color: #5fb136;">个</span></span>
    </div>
  </div>
  <div class="card_store2">
    <div class="picture_warn">
    </div>
    <div>
      <span class="every_card_store">在17种商品中</span></br>
      <span class="data_index_store_warn">品类不足<span class="store_number">6</span><span class="data_unit">个</span></span>
    </div>
  </div>
  <div class="card_store3">
    <div class="picture_primary">
    </div>
    <div>
      <span class="every_card_store">在17种商品中</span><br/>
      <span class="data_index_store_primary">品类充足<span class="store_number">11</span><span class="data_unit">个</span></span>
    </div>
  </div>
</div>

<%= render 'store' %>
